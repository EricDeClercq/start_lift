   rshz\r\n}\r\n\r\n//% weight=100 color=#0fbc11 icon=\"\\uf10c\"\r\nnamespace Bitwise {\r\n    //% blockId=\"id_aox\" block=\"%op1 | %op | %op2\"\r\n    export function fn_aox(op1: number, op: bwop, op2: number): number {\r\n        if (op == bwop.and)\r\n            return op1 & op2\r\n        else if (op == bwop.or)\r\n            return op1 | op2\r\n        else if (op == bwop.xor)\r\n            return op1 ^ op2\r\n        else if (op == bwop.rshs)\r\n            return op1 >> op2\r\n        else if (op == bwop.lshz)\r\n            return op1 << op2\r\n        else\r\n            return op1 >>> op2\r\n    }\r\n    \r\n    //% blockId=\"id_not\" block=\"NOT | %op1\"\r\n    export function fn_not(op1: number): number {\r\n        return op1 ^ 0xffff\r\n    }\r\n}","README.md":"","pxt.json":"{\n    \"name\": \"Start_Lift\",\n    \"description\": \"\",\n    \"dependencies\": {\n        \"core\": \"*\",\n        \"radio\": \"*\",\n        \"microphone\": \"*\",\n        \"SSD1306_OLED\": \"github:tinkertanker/pxt-oled-ssd1306#v2.0.18\"\n    },\n    \"files\": [\n        \"main.blocks\",\n        \"main.ts\",\n        \"custom.ts\",\n        \"README.md\"\n    ],\n    \"preferredEditor\": \"blocksprj\"\n}\n"}}],"shares":[],"lastSaveTime":1769958818771}